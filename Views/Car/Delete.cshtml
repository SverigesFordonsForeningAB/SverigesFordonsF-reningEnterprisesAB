@model SverigesFordonsFöreningEnterprisesAB.Models.Car
<h2>Delete Car</h2>
<h3>Are you sure you want to delete this Car</h3>
<div>
	<h4>Car</h4>
	<hr />
	<dl class="dl-horizontal">
		<dt>Brand</dt>
		<dd>@Model.Brand</dd>
		<dt>Model</dt>
		<dd>@Model.Model</dd>
		<dt>CarPrice</dt>
		<dd>@Model.CarPrice</dd>
	</dl>
	<form asp-action="Delete" method="post">
		<input type="hidden" asp-for="CarId" />
		<input type="submit" value="Delete" class="btn btn-danger" />
		<a asp-action="Index" class="btn btn-success">Cancel</a>
	</form>
</div>