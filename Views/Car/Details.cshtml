@model SverigesFordonsFöreningEnterprisesAB.Models.Car

<h2>Car details</h2>
<div>
    <h4>Car</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID
        </dt>
        <dd class="col-sm-10">
            @Model.CarId
        </dd>
        <dt class="col-sm-2">
            Model
        </dt>
        <dd class="col-sm-10">
            @Model.Model
        </dd>
        <dt class="col-sm-2">
            Brand
        </dt>
        <dd class="col-sm-10">
            @Model.Brand
        </dd>
        <dt class="col-sm-2">
            CarPrice
        </dt>
        <dd class="col-sm-10">
            @Model.CarPrice
        </dd>
    </dl>
    <a asp-action="Edit" class="btn btn-primary" asp-route-id="@Model.CarId">Edit</a>
    <a asp-action="Index" class="btn btn-success">Back to list</a>
</div>