@model SverigesFordonsFöreningEnterprisesAB.Models.Customer
<h2>Delete customers</h2>
<h3>Are you sure you want to delete this customers</h3>
<div>
	<h4>Customers</h4>
	<hr />
	<dl class="dl-horizontal">
		<dt>FirsName</dt>
		<dd>@Model.FirstName</dd>
		<dt>LastName</dt>
		<dd>@Model.LastName</dd>
		<dt>Phon</dt>
		<dd>@Model.Phone</dd>
		<dt>SocialSecurityNumber</dt>
		<dd>@Model.SocialSecurityNumber</dd>
		<dt>Adress</dt>
		<dd>@Model.Address</dd>
		<dt>Email</dt>
		<dd>@Model.Email</dd>
	</dl>
	<form asp-action="Delete" method="post">
		<input type="hidden" asp-for="CustomerId" />
		<input type="submit" value="Delete" class="btn btn-danger" />
		<a asp-action="Index" class="btn btn-success">Cancel</a>
	</form>
</div>